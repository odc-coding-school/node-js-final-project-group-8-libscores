<%- include ("partials/header", { title: title }) %>
    <div class="">
      <section class="dashcontainer">
        <%- include ("partials/leagues_detail.ejs") %>
          <main class="main_display">
            <nav class="border-bottom">
                <div class="">
                  <div class="qualification_group">
                    <div class="qualification_link">
                      <a href="" class="qualification">
                        <img class=" qualification_img" src="./images/ELWA-Utd-logo-128x128-removebg-preview.png" alt="">
                        <div>
                          <h5 class="text_color">League Stage</h5>
                          <p>Liberia</p>
                        </div>
                      </a>
                    </div>
                    <div class="qualification_link">
                     <button type="button" onclick="selsect_favorite">
                      <i class="fa-regular fa-star fs-2 text_color"></i>
                     </button>
                    </div>
                  </div>
                </div>
                <div class="" id="navbarCollapse">
                  <div class="d-flex">
                    <a href="/league1_overview/" class="nav-item nav-link fs-4 fw-bold text-light">Overview</a>
                    <a href="//league1_matches/" class="nav-item nav-link fs-4 fw-bold text-light">Matches</a>
                    <a href="/league1_table/" class="nav-item nav-link fs-4 fw-bold text-light">Table</a>
                </div>
              </div>
            </nav>
          <section class="main_content-sec">
            <ul class="nav flex-column gap-2">
              <li class="nav-item">
                  <a class="nav-item nav-link sidebar_headlings text_color d-flex justify-content-between align-items-center" href="#">
                    <span>FIXTURES</span>
                      <i class="fa-solid fa-angle-right"></i>
                  </a>
                  
                  <% matches.forEach(match => { %>
                  <fieldset class="main_content d-flex justify-content-between align-items-center">
                    <a href="#" class="main_content_link d-flex text_color justify-content-between align-items-center text-decoration-none">
                    <div class="d-flex lign-items-center gap-4 ms-2">
                      <div class="d-flex align-items-center flex-column">
                        <p><%= match.match_date %></p>
                        <p><%= match.status %></p>
                      </div>
                      <div class="main_content_teams">
                        <div class="d-flex gap-2">
                          
                          <% if (match.home_team_logo) { %>
                            <img src="data:image/png;base64,<%= match.home_team_logo.toString('base64') %>" alt="League Logo" class="sidebar_imgs">
                          <% } else { %>
                            <img src="./images/teams_flag/generic-logo-128x128.jpg" alt="" class="sidebar_imgs">
                          <% } %>
                          <p><%= match.home_team_name %></p>
                        </div>
                        <div class="d-flex gap-2">
                          <% if (match.away_team_logo) { %>
                            <img src="data:image/png;base64,<%= match.away_team_logo.toString('base64') %>" alt="League Logo" class="sidebar_imgs">
                          <% } else { %>
                            <img src="./images/teams_flag/generic-logo-128x128.jpg" alt="" class="sidebar_imgs">
                          <% } %>
                          <p><%= match.away_team_name %></p>
                        </div>
                      </div>
                    </div>
                    
                   </a>
                   <div class="contant me-2">
                    <button type="button" onclick="selsect_favorite">
                   <i class="fa-regular fa-star fs-2 text_color"></i>
                  </button>
                 </div>
                </fieldset>
              <% }) %>
              
              <% countymatches.forEach(countymatches => { %>
                <fieldset class="main_content">
                  <a href="/<%= countymatches.home_county_name %>vs/<%= countymatches.away_county_name %>" class="main_content_link d-flex justify-content-between align-items-center text-decoration-none">
                  <div class="d-flex lign-items-center gap-4 ms-2">
                    <div class="d-flex align-items-center">
                      <p><%= countymatches.match_status %></p>
                    </div>
                    <div class="main_content_teams">
                      <div class="d-flex gap-2">
                        <% if (countymatches.home_county_logo) { %>
                          <img src="data:image/png;base64,<%= countymatches.home_county_logo.toString('base64') %>" alt="Home Team Logo" class="sidebar_imgs">
                        <% } else { %>
                          <img src="./images/teams_flag/generic-logo-128x128.jpg" alt="" class="sidebar_imgs">
                        <% } %>
                        <p class="team-p"><%= countymatches.home_county_name %></p>
                      </div>
                      <div class="d-flex gap-2">
                        <% if (countymatches.away_county_logo) { %>
                          <img src="data:image/png;base64,<%= countymatches.away_county_logo.toString('base64') %>" alt="Away Team Logo" class="sidebar_imgs">
                        <% } else { %>
                          <img src="./images/teams_flag/generic-logo-128x128.jpg" alt="" class="sidebar_imgs">
                        <% } %>
                        <p class="team-p"><%= countymatches.away_county_name %></p>
                      </div>
                    </div>
                  </div>
                  <div class="contant me-2">
                    <i class="fa-regular fa-star fs-2"></i>
                  </div>
                 </a>
              </fieldset>
              <% }) %>
          </li>
        </ul>
