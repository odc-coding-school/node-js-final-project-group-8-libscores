<aside class="main-aside">
<div class="position-sticky">
<ul id="detail_list" class="nav flex-column gap-2">
  <div class="d-flex justify-content-between align-items-center">
      <a class="nav-item nav-link sidebar_headlings" href="/">
            <i class="fa-solid fa-angle-left fs-2 fw-2 text_color"></i>
      </a>
      <span class="text_color fs-4 fw-4"><%= leaguedata.league_name %></span>
  </div>
  <div class="">
      <a href="/league1_overview" class="text-decoration-none">
          <fieldset class="aside_content">
              <div class="contant">
                <% if (leaguedata && leaguedata.league_logo) { %>
                    <img src="data:image/png;base64,<%= leaguedata.league_logo.toString('base64') %>" alt="League Stage" class="sidebar_imgs rounded-circle">
                  <% } else { %>
                    <p>LCS</p>
                  <% } %>
                  <div class="text_content">League Stage</div>
              </div>
          </fieldset>
      </a>
      <a href="/league1_qualification" class="text-decoration-none">
          <fieldset class="aside_content">
              <div class="contant">
                <% if (leaguedata && leaguedata.league_logo) { %>
                    <img src="data:image/png;base64,<%= leaguedata.league_logo.toString('base64') %>" alt="League Stage" class="sidebar_imgs rounded-circle">
                  <% } else { %>
                    <p>LCS</p>
                  <% } %>
                  <div class="text_content">Qualification</div>
              </div>
          </fieldset>
      </a>
  </div>
</ul>
</div>
</aside>
<button id="back_to_default" class="back_to_default_btn" style="display: none;"><i class="fa-solid fa-arrow-left"></i></button>
